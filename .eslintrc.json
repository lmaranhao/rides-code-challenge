{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "import/extensions": [
            "error",
            "never"
        ],
        "import/no-unresolved": "off"
    },
    "ignorePatterns": [
        "rides_mongo.json",
        "./build/*",
        "./node_modules/*"
    ]
}